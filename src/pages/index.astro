   property="og:image"
   content="https://res.cloudinary.com/drxurk7lu/image/upload/v1711831025/newslettersignup/Newsletter_sign-up_form_1_njyw85.jpg"
  />
  <meta
   property="og:url"
   content="https://newsletter-signup-rosy.vercel.app/"
  />
